#: help.xml:593
msgid "SPECIAL BOOT PARAMETERS - INSTALLATION SYSTEM"
msgstr "SPECIÁLIS RENDSZERINDÍTÓ PARAMÉTEREK - TELEPÍTŐ"

#: help.xml:607
msgid "These parameters control how the installer works."
msgstr "Ezek a paraméterek a telepítő működését szabályozzák."

#: help.xml:610
msgid "These parameters control how the bootstrap system works."
msgstr "Ezek a paraméterek a rendszerindító működését szabályozzák."

#: help.xml:615
msgid "RESULT"
msgstr "EREDMÉNY"

#: help.xml:616
msgid "PARAMETER"
msgstr "PARAMÉTER"

#: help.xml:619
msgid "Verbose debugging"
msgstr "Bőbeszédű hibakeresés"

#: help.xml:620
msgid "<userinput>DEBCONF_DEBUG=5</userinput>"
msgstr "<userinput>DEBCONF_DEBUG=5</userinput>"

#: help.xml:624
msgid "Debug boot sequence"
msgstr "Rendszerindítási foylamat hibakeresése"

#: help.xml:625
