msgid ""
"<userinput>reserve=<replaceable>iobase</replaceable>,<replaceable>extent</"
"replaceable></userinput><optional><userinput>,<replaceable>...</"
"replaceable></userinput></optional>"
msgstr ""
"<userinput>reserve=<replaceable>iobase</replaceable>,<replaceable>extent</"
"replaceable></userinput><optional><userinput>,<replaceable>...</"
"replaceable></userinput></optional>"

#: help.xml:471
