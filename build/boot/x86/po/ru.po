#: help.xml:457
msgid "<userinput>floppy.floppy=thinkpad</userinput>"
msgstr "<userinput>floppy.floppy=thinkpad</userinput>"

#: help.xml:461
msgid "IBM Pentium Microchannel"
msgstr "IBM Pentium Microchannel"

#: help.xml:462
msgid "<userinput>mca-pentium no-hlt</userinput>"
msgstr "<userinput>mca-pentium no-hlt</userinput>"

#: help.xml:466
msgid "Protect I/O port regions"
msgstr "Защита портов I/O"

#: help.xml:467
